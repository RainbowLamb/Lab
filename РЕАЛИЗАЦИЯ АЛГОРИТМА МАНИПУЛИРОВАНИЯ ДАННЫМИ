#include <stdio.h>
#include <stdlib.h>

int main()

{
    int m, n;
    printf("enter massive size\n");
    scanf("%d%d", &m, &n);
    if (m>=3, n>=4);
    {
        int massive [m][n];
        int i, j;
        for (i=0; i<m; i++)
        {
            for (j=0; j<n; j++)
            {
                massive [i][j]=rand()%100+1;
                printf ("%d\t",massive [i][j]);
            }
            printf("\n");
        }
    }
    if (m<3, n<4)
        printf('Error');
    return 0;

}
